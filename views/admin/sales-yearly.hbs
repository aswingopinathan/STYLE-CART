<h4 class="page-title text-center">Weekly Sales Report-{{yearValue}}</h4>
<div class="row">
    <div class="col-12">
        <div class="card m-b-30">
            <div class="card-body">
                <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap"
                    style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                    <thead>
                        <tr>
                            <th>Years</th>
                            <th>Revenue</th>
            
                           
                        </tr>
                    </thead>

                    <tbody>
                        {{#each yearlySalesReport}}
                        <tr>
                            <td>{{this.year}}</td>
                            <td>{{this.grandTotal}}</td>
                        </tr>
                         {{/each}}

                    </tbody>
                </table>

            </div>
        </div>
    </div> <!-- end col -->
</div> <!-- end row -->

